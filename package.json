{
  "name": "customerapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "android-uat": "react-native run-android --variant=uatDebug --appIdSuffix=uat react-devtools",
    "android-prd": "react-native run-android --variant=prdDebug",
    "ios": "ENVFILE=.env.dev react-native run-ios --scheme 'dev' --simulator='iPhone 8'",
    "ios-uat": "ENVFILE=.env.uat react-native run-ios --scheme 'uat' --simulator='iPhone 8'",
    "ios14-uat": "ENVFILE=.env.uat react-native run-ios --scheme 'uat' --simulator='iPhone 14'",
    "ios-prd": "ENVFILE=.env.prd react-native run-ios --simulator='iPhone 8'",
    "start": "react-native start",
    "test": "jest src --transformIgnorePatterns",
    "lint": "eslint .",
    "start-reset": "node node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "release-dev-android": "ENVFILE=.env.dev cd android && ./gradlew assembleRelease && cd ..",
    "release-dev-android-win": "SET ENVFILE=.env.dev && cd android && gradle assembleRelease && cd ..",
    "release-uat-android": "ENVFILE=.env.uat cd android && ./gradlew assembleUatRelease && cd ..",
    "release-uat-android-win": "SET ENVFILE=.env.uat && cd android && gradle assembleUat && cd ..",
    "release-prd-android": "ENVFILE=.env.prd cd android && ./gradlew assemblePrdRelease && cd ..",
    "release-prd-android-win": "SET ENVFILE=.env.prd && cd android && gradle assemblePrd && cd ..",
    "codepush-uat-rnd-android": "appcenter codepush release-react -a app.dev-ifg-life.id/Customer-Apps-RnD -d uat -t",
    "codepush-uat-rnd-ios": "appcenter codepush release-react -a app.dev-ifg-life.id/Customer-Apps-RnD-1 -d uat -t",
    "codepush-general-android": "appcenter codepush release-react -a app.dev-ifg-life.id/Customer-Apps-General -d",
    "codepush-general-ios": "appcenter codepush release-react -a app.dev-ifg-life.id/Customer-Apps-General-1 -d"
  },
  "dependencies": {
    "@datadog/mobile-react-native": "^1.4.0",
    "@datadog/mobile-react-navigation": "^1.4.0",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@pankod/react-native-store-rating": "^1.2.0",
    "@react-native-clipboard/clipboard": "^1.10.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/checkbox": "^0.5.12",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-native-firebase/analytics": "^14.11.0",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/crashlytics": "^14.11.0",
    "@react-native-firebase/dynamic-links": "^14.11.0",
    "@react-native-firebase/firestore": "^14.11.0",
    "@react-native-google-signin/google-signin": "^7.2.2",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "add": "^2.0.6",
    "appcenter": "^4.4.3",
    "appcenter-analytics": "^4.4.3",
    "appcenter-crashes": "^4.4.3",
    "axios": "^0.27.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "form-data": "^4.0.0",
    "jail-monkey": "^2.7.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.2",
    "numbro": "^2.3.6",
    "react": "17.0.2",
    "react-native": "0.67.4",
    "react-native-aaiios-liveness-sdk": "link:./react-native-aaiios-liveness-sdk",
    "react-native-app-intro-slider": "^4.0.4",
    "react-native-appsflyer": "^6.8.0",
    "react-native-biometrics": "^2.2.0",
    "react-native-blob-util": "^0.16.2",
    "react-native-camera-kit": "^13.0.0",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.4.5",
    "react-native-contacts": "^7.0.5",
    "react-native-dash": "^0.0.11",
    "react-native-device-info": "^9.0.2",
    "react-native-document-picker": "^8.1.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fbsdk-next": "^8.0.2",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-image-crop-picker": "^0.38.0",
